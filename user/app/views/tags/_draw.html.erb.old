function draw()
{

  for (var i=0; i < MYAPP.banners.length; i++)
  {

    MYAPP.drow_position_x = MYAPP.cell_width * (i % 3);
    MYAPP.drow_position_y = MYAPP.cell_height * Math.floor(i / 3) + MYAPP.y;
    var mybi = MYAPP.banners[i];
    var mdpx = MYAPP.drow_position_x | 0;
    var mdpy = MYAPP.drow_position_y | 0;

    if (mdpy + MYAPP.cell_height >= 0 && mdpy <= canvas.height)
    //表示される位置に少しでもある場合
    {
      context2D.drawImage(mybi, mdpx, mdpy);
     // context2D.clearRect(mdpx, mdpy + mybi.height, mybi.width, 0);
    }

    mdpy += MYAPP.loop;
    if (mdpy <= canvas.height)
    //次がすでに表示される位置にある場合
    {
      context2D.drawImage(mybi, mdpx, mdpy);
      context2D.clearRect(mdpx, mdpy + mybi.height, mybi.width, 2);
    }

// if (MYAPP.loop < canvas.height)
//     {
//       mdpy += MYAPP.loop;
//       if (mdpy <= canvas.height)
//       {
//         context2D.drawImage(mybi, mdpx, mdpy);
//         context2D.clearRect(mdpx, mdpy + mybi.height, mybi.width, 2);
//       }
//     }
  }

  MYAPP.y -= 4;//キャリアによってここを変えることができれば

  if (MYAPP.y <= - MYAPP.loop) MYAPP.y = 0;
}
